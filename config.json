{
    "ipfs": {
        "enabled": true,
        "service": "lighthouse",
        "config": {
            "lighthouse": {
                "apiKey": "your-lighthouse-key"
            },
            "thirdweb": {
                "secretKey": "your-thirdweb-key"
            }
        }
    },
    "attributesFolders": {
        "Background": "./layers/Background",
        "Skin": "./layers/Skin",
        "Clothes": "./layers/Clothes",
        "Mouth": "./layers/Mouth",
        "Eyes": "./layers/Eyes",
        "Head": "./layers/Head"
    },
    "outputFolder": "./output",
    "imageSize": 500,
    "nftCount": 10,
    "nftPrefix": "nft_name",
    "policyID": "your-policy-id",
    "useRules": true
}
